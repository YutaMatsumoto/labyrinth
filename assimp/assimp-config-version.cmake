set( ASSIMP_PACKAGE_VERSION "3.0.1264" )
if( "${ASSIMP_PACKAGE_FIND_VERSION}" VERSION_EQUAL "3.0.1264")
  set(ASSIMP_PACKAGE_VERSION_EXACT 1)
endif()
if( "${ASSIMP_PACKAGE_FIND_VERSION_MAJOR}.${ASSIMP_PACKAGE_FIND_VERSION_MINOR}" EQUAL "3" )
  set(ASSIMP_PACKAGE_VERSION_COMPATIBLE 1)
elseif( "${ASSIMP_PACKAGE_FIND_VERSION_MAJOR}" EQUAL "3" )
  # for now backward compatible if minor version is less
  if( ${ASSIMP_PACKAGE_FIND_VERSION_MINOR}  LESS 0 )
    set(ASSIMP_PACKAGE_VERSION_COMPATIBLE 1)
  endif()
endif()
set( ASSIMP_STATIC_LIB "OFF")
